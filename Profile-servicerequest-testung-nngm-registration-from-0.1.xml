        <map value=".code" />
      </mapping>
      <mapping>
        <identity value="quick" />
        <map value="Procedure.procedureCode" />
      </mapping>
    </element>
    <element id="ServiceRequest.Quantity">
      <path value="ServiceRequest.quantity[x]" />
      <short value="Service amount" />
